{
  "name": "@nestjs/azure-database",
  "version": "1.0.0",
  "description": "The Azure Table Storage module for Nest framework (node.js)",
  "author": {
    "name": "Wassim Chegham",
    "email": "github@wassim.dev",
    "url": "https://twitter.com/manekinekko"
  },
  "main": "./dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjs/azure-database.git"
  },
  "scripts": {
    "test": "jest --passWithNoTests",
    "precommit": "lint-staged",
    "prettier": "prettier src/**/*.ts --write && git status",
    "build": "rimraf dist && npm run build:lib && npm test",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "build:lib": "tsc -p tsconfig.json",
    "prepare": "npm run build",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0"
  },
  "dependencies": {
    "@azure/cosmos": "^3.4.2",
    "@azure/ms-rest-js": "^2.0.4",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "azure-storage": "^2.10.3"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@nestjs/testing": "7.6.5",
    "@types/jest": "26.0.20",
    "@types/node": "11.13.21",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "@typescript-eslint/parser": "4.14.0",
    "dotenv": "^8.1.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "supertest": "6.1.2",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  }
}
