{
  "name": "@nestjs/azure-database",
  "version": "2.1.0",
  "description": "The Azure Table Storage module for Nest framework (node.js)",
  "author": {
    "name": "Wassim Chegham",
    "email": "github@wassim.dev",
    "url": "https://twitter.com/manekinekko"
  },
  "main": "./dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjs/azure-database.git"
  },
  "scripts": {
    "test": "jest --passWithNoTests",
    "precommit": "lint-staged",
    "prettier": "prettier src/**/*.ts --write && git status",
    "build": "rimraf dist && npm run build:lib && npm test",
    "lint": "eslint '{lib,tests}/**/*.ts' --fix",
    "build:lib": "tsc -p tsconfig.json",
    "prepare": "npm run build",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.0 || ^8.0.0",
    "@nestjs/core": "^7.0.0 || ^8.0.0"
  },
  "dependencies": {
    "@azure/cosmos": "^3.11.3",
    "@azure/ms-rest-js": "^2.5.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "azure-storage": "^2.10.4"
  },
  "devDependencies": {
    "@commitlint/cli": "13.2.0",
    "@commitlint/config-angular": "13.2.0",
    "@nestjs/testing": "8.0.8",
    "@types/jest": "27.0.2",
    "@types/node": "14.17.20",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "husky": "7.0.2",
    "jest": "27.2.3",
    "lint-staged": "11.1.2",
    "prettier": "2.4.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "supertest": "6.1.6",
    "ts-jest": "27.0.5",
    "typescript": "4.4.3"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  }
}