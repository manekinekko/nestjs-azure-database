{
  "name": "@nestjs/azure-database",
  "version": "2.3.0",
  "description": "The Azure Table Storage module for Nest framework (node.js)",
  "author": {
    "name": "Wassim Chegham",
    "email": "github@wassim.dev",
    "url": "https://twitter.com/manekinekko"
  },
  "main": "./dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjs/azure-database.git"
  },
  "scripts": {
    "test": "jest --passWithNoTests",
    "precommit": "lint-staged",
    "prettier": "prettier src/**/*.ts --write && git status",
    "build": "rimraf dist && npm run build:lib && npm test",
    "lint": "eslint '{lib,tests}/**/*.ts' --fix",
    "build:lib": "tsc -p tsconfig.json",
    "prepare": "npm run build",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "@nestjs/core": "^7.0.0 || ^8.0.0 ||^9.0.0"
  },
  "dependencies": {
    "@azure/cosmos": "^3.16.2",
    "@azure/ms-rest-js": "^2.6.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "azure-storage": "^2.10.7"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-angular": "17.6.6",
    "@nestjs/testing": "9.1.6",
    "@types/jest": "29.5.3",
    "@types/node": "18.16.19",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "dotenv": "16.3.1",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "husky": "8.0.3",
    "jest": "29.6.1",
    "lint-staged": "13.2.3",
    "prettier": "3.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  }
}
